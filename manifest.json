{
  "manifest_version": 2,
  "name": "Clever Crawler Extension",
  "version": "0.0.1",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "browser_action": {
    "default_icon": "assets/images/computer.png",
    "default_title": "Clever Crawler X",
    "default_popup": "html/popup.html"
  }
}
